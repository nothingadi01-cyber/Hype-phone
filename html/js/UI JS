function hasAccess(job, allowed) {
    return allowed.includes(job);
}
function postFeed(msg, img=null){
  fetch(`https://${GetParentResourceName()}/newPost`,{
    method:'POST',
    headers:{'Content-Type':'application/json'},
    body:JSON.stringify({message:msg,image:img})
  })
}
function wipePhone(){
  fetch(`https://${GetParentResourceName()}/wipe`,{
    method:'POST',
    headers:{'Content-Type':'application/json'},
    body:JSON.stringify({ reason:'remote' })
  })
}
